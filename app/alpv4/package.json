{
  "name": "alpv4",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {},
  "devDependencies": {
    "@ui5/cli": "^3",
    "@sap/ux-ui5-tooling": "^1",
    "@sap/ux-specification": "^1",
    "eslint": "^8",
    "@sap/eslint-plugin-ui5-jsdocs": "^2",
    "@sapui5/ts-types": "^1",
    "eslint-plugin-fiori-custom": "^2",
    "@babel/eslint-parser": "^7",
    "@sap-ux/ui5-middleware-fe-mockserver": "^2"
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#testalpv4-tile\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#testalpv4-tile\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "lint": "eslint ./ --rulesdir ./node_modules/eslint-plugin-fiori-custom/lib/rules/",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#testalpv4-tile\"",
    "int-test": "fiori run --config ./ui5-mock.yaml --open \"test/integration/opaTests.qunit.html\"",
    "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\""
  },
  "ui5": {
    "dependencies": [
      "@sap/ux-ui5-tooling",
      "@sap-ux/ui5-middleware-fe-mockserver"
    ]
  },
  "sapux": true,
  "sapuxLayer": "CUSTOMER_BASE"
}
